generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL_DEV")
}

model bookings {
  id                 String   @id @unique
  localizer          String
  status             String
  booking_date       String
  company_id         String?
  accommodation_code String
  arrival            DateTime
  departure          DateTime
  external_data      String[]
  creation_date      DateTime @default(now())
  updated_at         DateTime
}
